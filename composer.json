{
    "name": "bboxlab/mosel",
    "description": "SDK for BT open APIs",
    "keywords": ["sdk", "open-api", "open api", "api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gaétan Darquié",
            "email": "gdarquie@bouyguestelecom.fr"
        },
        {
            "name": "François Gette",
            "email": "fgette@bouyguestelecom.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/http-client": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/validator": "^5.4",
        "doctrine/annotations": "^1.13",
        "symfony/property-access": "^5.4",
        "symfony/cache": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Bboxlab\\Mosel\\": "src/",
            "Bboxlab\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1"
    }
}
